{
  input: "cr75-rdnd-sd2.ang", // input ang file
  //input: "gg_0.ang",    // use saved (cropped) ang file
  //input: 15.5,          // circle of given radious (in cells)
  //input: [75, 75, 10],  // random microstructure (x, y, orientations)
  crop: [0, 0],       // crop region (um), [0, 0] to not crop
  ci: 0.1,            // confidence index
  iq: 1E3,            // image quality
  out: "gg_",         // output files (out0.ang and out1.ang)
  tout: 60,           // time frequency to write output (0 for all, 1E99 for none)
  da: 2.5,            // delta angle (degrees)
  lagb: 1.0,          // low angle grain boudary (degrees)
  hagb: 15,           // high angle grain boundary (degrees)
  G0: 0.5,            // boundary energy of hagb in Read-Shockley equation (J/m^2)
  GF: [1.25, 1.5],    // boundary energy factor at triple and quadruple points
  M0: 2.84E-6,        // mobility for hagb in Humphrey's law (m^4/J s)
  Qg: 140E3,          // activation energy for boundary mobility (J/mol)
  A: 5,               // factor in Humphrey's law
  n: 4,               // exponent in Humphrey's law
  Rp: 1E-4,           // random perturbation of transformation energies
  dx: 0.0375E-6,      // cell size (m)
  tmin: 1E-0,         // minimum step time (s)
  tmax: 300,          // simulation time (s)
  T: 773,             // temperature (K)
  seed: 1,            // seed for random numbers (0 for random)
}
